@model IEnumerable<string>

  
@Html.ActionLink("Home", "List", "Product", null,
     new { @class = "btn btn-outline-primary btn-block " +   (ViewBag.categorie == null ? "active" : "") })



@foreach (var link in Model)
{
        @Html.RouteLink(link, new
   {
       controller = "Product",
       action = "List",
       category = link,
       page = 1
   },
  new { @class = "btn btn-outline-primary btn-block " + (link == ViewBag.categorie ? "active" : "") }
    )





}
